<?xml version="1.0" encoding="utf-8"?>
<swiz:BeanProvider xmlns:fx="http://ns.adobe.com/mxml/2009" 
				   xmlns:s="library://ns.adobe.com/flex/spark"
				   xmlns:swiz="http://swiz.swizframework.org" 
				   xmlns:controllers="main.flex.com.sxm.weatherMap.core.controllers.*" 
				   xmlns:models="main.flex.com.sxm.weatherMap.core.models.*">
	
	<fx:Script>
		<![CDATA[
			import main.flex.com.sxm.weatherMap.core.models.WeatherReportModel;
			import main.flex.com.sxm.weatherMap.core.models.presentation.WeatherReportPresentationModel;
		]]>
	</fx:Script>
	
	<fx:Declarations>
		<!-- model -->
		<models:WeatherReportModel id="weatherReportModel"/>
		
		<!-- view -->
		<swiz:Prototype type="{WeatherReportPresentationModel}" />	
		
		<!-- controller -->
		<controllers:WeatherReportController id="weatherReportController"/>
		
	</fx:Declarations>
	
</swiz:BeanProvider	>
